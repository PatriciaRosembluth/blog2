<h1>Pagina Principal</h1>
<table class="table table-striped">
	<tr>
		<th>Id</th>
		<th>Titulo</th>
		<th>Texto</th>
		<th>Fecha Creacion</th>
		<th>Fecha Modificacion</th>
		<th>Likes</th>
	</tr>
<% @posts.each do |post| %>
<tr>
	<td>
		<%= post.id %>
	</td>
	<td>
		<a href="/posts/<%= post.id %>"><%= post.title %></a>
	</td>
	<td>
		<%= post.text %>
	</td>
	<td>
		<%= post.created_at %>
	</td>
	<td>
		<%= post.updated_at %>
	</td>
	<td>
		<%= post.likes %>
	</td>
	<td>
		<a class="btn btn-default" href="/posts/likes/<%= post.id %>" role="button">Like</a>
		<a class="btn btn-default" href="/posts/dislikes/<%= post.id %>" role="button">Dislike</a>
	</td>
</tr>
<%end%>
</table>
<br>



