<h1>Pagina Principal</h1>
<table>
	<tr>
		<th>Titulo</th>
		<th>Texto</th>
		<th>Fecha</th>
		<th></th>
	</tr>
<% @posts.each do |post| %>
<tr>
	<td>
		<a href="/posts/<%= post.id %>"><%= post.title %></a>
	</td>
	<td>
		<%= post.text %>
	</td>
	<td>
		<%= post.created_at %>
	</td>
</tr>
<%end%>
</table>
<br>
<%= link_to "Pagina Administradora", posts_path%> |
<a href="/posts/report">Reportes</a>


